// 声明CSS常用变量
asideWidth = 210px //logo部分宽度
contentWidth = 85% //内容部分宽度
borderStyle = 1px solid #e4e4e4 //边框样式
mainColor = #379aff //蓝色主色值
fontColor = #324b69//字体色值
fontBold = 600//字体磅数


body
  font 14px/1.5 "PingFang SC","微软雅黑","Microsoft YaHei",Helvetica,"Helvetica Neue",Tahoma,Arial,sans-serif
  margin 0
  -webkit-font-smoothing antialiased
  color fontColor
  line-height 1.4
  background-color #eff0f4
  font-size 14px
  height 100%

.icon 
  width 1em 
  height 1em
  vertical-align -0.15em
  fill currentColor
  overflow hidden
    

::selection
  background #32aab7
  color #fff

html
  box-sizing border-box
  background-color #eff0f4!important


*,*:before,*:after
  box-sizing inherit
  -webkit-overflow-scrolling  touch

a:focus,a:active
  outline none


a,a:focus,a:hover
  cursor pointer
  color inherit
  text-decoration none


.clearfix
  &:after
    visibility hidden
    display block
    font-size 0
    content " "
    clear both
    height 0

//vue router transition css
.fade-enter-active,.fade-leave-active
  transition all .2s ease


.fade-enter,.fade-leave-active
  opacity 0


//main-container全局样式
.app-main
  min-height 100%


.app-container
  padding: 30px


.svg-icon
  width 1em
  height 1em
  vertical-align -0.15em
  fill currentColor
  overflow hidden

li
  list-style none
ul
  margin 0
  padding 0
input
  outline none

.no-content
  color #b5b5b5
  i
    font-size 100px
